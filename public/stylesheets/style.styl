body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

// tab content
.inner-search-panel
  height: 70px

// search_panel inner p
.search_panel_input_container
  padding: 5px

input[type="file"]
  display: inline !important

// overlay
#overlay
  height: 100%
  position: absolute
  width: 100%
  background-color: grey
  z-index: 10000
  left: 0
  top: 0
  opacity: 0.3

// dialog
#message
  width: 300px
  height: 50px
  vertical-align: middle
  margin-left: auto
  margin-right: auto
  margin-top: 30%
  z-index: 10005
  background-color: whitesmoke
  text-align: center
  color: red
  padding: 16px