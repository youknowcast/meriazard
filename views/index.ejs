<% include head_template %>
    <h1><%= title %></h1>
    <form action="users" method="post" class="form-inline" role="form">
      <div class="form-group">
        <label class="sr-only" for="input_id">id</label>
        <input type="text" class="form-control" name="input_id" id="input_id" placeholder="id or email address" />
      </div>
      <div class="form-group">
        <label class="sr-only" for="input_passwd">password</label>
        <input type="text" class="form-control" name="input_passwd" id="input_passwd" placeholder="passwd を入力" />
      </div>
    </div>
    <button type="submit" class="btn btn-default">Sign in</button>
    </form>
<% include foot_template %>